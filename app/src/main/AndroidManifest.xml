<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.storybook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StoryBook">
        <activity
            android:name="com.example.ticTacToeGame.Activity.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.example.ticTacToeGame.Activity.TimerActivity"
            android:exported="false" />
        <activity
            android:name="com.example.ticTacToeGame.Activity.TicTacToeActivity"
            android:exported="false" />
        <activity
            android:name="com.example.ticTacToeGame.Activity.GuessTheNumberActivity"
            android:exported="false" />
        <activity
            android:name="com.example.ticTacToeGame.Activity.GuessThePhoneNumberActivity"
            android:exported="false" />
        <activity
            android:name="com.example.ticTacToeGame.Activity.LoadingActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"

            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="com.example.ticTacToeGame.Services.TimerService"></service>
    </application>

</manifest>